<!-- Want to have a form that posts data to the server -->
<!-- Posts: email, password -->
<form action="/users" method="post">
    <label for="email">Email</label>
    <input type="text" id="email" name="email" placeholder="Email address">
    
    <label for="password">Password</label>
    <input type="password" id="password" name="password" placeholder="Password">

    <button>Register</button>
</form>

<br>

<!-- Run a function; if the user hasn't inputted something: don't display -->
<!-- IF the user's input email isn't unique, don't display -->
<!-- IF the user's input email is unique, display it -->
<!-- User's input email... req.body.email -->

<% if (!isUniqueEmail) { %>
    <p><%= message %></p>
<% } %>






